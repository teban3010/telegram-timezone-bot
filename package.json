  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "npm-run-all --parallel dev ngrok",
    "build": "next build",
    "start": "next start",
    "test": "jest --verbose --color",
    "coverage": "jest --coverage --verbose --color",
    "ngrok": "ngrok http 3000",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format test build"
  },
  "dependencies": {
    "date-fns": "^2.21.3",
    "kvdb.io": "^1.0.5",
    "next": "10.2.0",
    "node-telegram-bot-api": "^0.53.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/node-telegram-bot-api": "^0.51.1",
    "@types/react": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "ngrok": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
